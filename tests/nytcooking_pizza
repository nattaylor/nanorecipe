          window.googletag.cmd.push(function() {
            window.googletag.display(`div-gpt-ad-804752003747430782-top-mobile`);
          });
        })();
    </script>
  </div>
</section>


        <div class="recipe-instructions">
            <section class="recipe-ingredients-wrap">
              <h3 class="recipe-instructions-heading">Ingredients</h3>

                <ul class="recipe-ingredients">
                  <li>
                    <span class="quantity">
                      1
                    </span>
                    <span class="ingredient-name">
                      12-inch round of pizza dough, stretched <a href="http://cooking.nytimes.com/recipes/1016230-robertas-pizza-dough">(see recipe)</a>
                    </span>
                  </li>
                  <li>
                    <span class="quantity">
                      3
                    </span>
                    <span class="ingredient-name">
                      tablespoons tomato sauce (see note)
                    </span>
                  </li>
                  <li>
                    <span class="quantity">
                      
                    </span>
                    <span class="ingredient-name">
                      Extra-virgin olive oil
                    </span>
                  </li>
                  <li>
                    <span class="quantity">
                      2 &frac34;
                    </span>
                    <span class="ingredient-name">
                      ounces fresh mozzarella
                    </span>
                  </li>
                  <li>
                    <span class="quantity">
                      4 to 5
                    </span>
                    <span class="ingredient-name">
                      basil leaves, roughly torn
                    </span>
                  </li>
                </ul>
